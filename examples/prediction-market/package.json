    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/minimatch": "^5.1.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "buffer": "^6.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
